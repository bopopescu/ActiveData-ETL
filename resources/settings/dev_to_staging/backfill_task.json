{
	"no_checks": true,
//	"range": {"min":2686776, "max":2726072},
	"limit": 1000,
	"es_filter":{"and": [
		{"gte":{"etl\\.timestamp":{"date":"2019-11-01"}}},
		{"lt":{"etl\\.timestamp":{"date":"2019-11-23"}}}
	]},
//	"git":{
//		"url":"https://github.com/klahnakoski/ActiveData-ETL.git",
//		"branch": "etl"
//	},
	"work_queue": {  //FILL THIS TO TRIGGER ETL ACTION
		"name": "active-data-index-es5",
		"$ref": "file://~/private.json#aws_credentials"
	},
	"source": {  //WHERE WE GET DATA FROM
		"$ref": "file://~/private.json#aws_credentials",
		"bucket": "active-data-task-cluster-normalized",
		"key_format": "t.a:b.c",
		"prefix": "tc."
	},
	"elasticsearch": {  // USE unittest TABLE
		"id_field": "etl.source.source.source.id",
		"host": "http://localhost",
		"port": 9201,
		"index": "unittest",
		"timeout": 300,
		"debug": true,
		"limit_replicas": false
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData",
			"User-Agent": "ActiveData-ETL"
		}
	},
	"debug": {
		"trace": true,
		"log": [
			{
				"log_type": "console"
			}
		]
	}
}
